{
  "$schema": "https://typedoc-plugin-markdown.org/schema.json",
  "entryPoints": ["./index.ts"],
  "name": "Wapi.js Docs",
  "plugin": ["typedoc-plugin-zod", "typedoc-plugin-markdown"],
  "fileExtension": ".mdx",
  "json": "./docs/api.json",
  "emit": "both",
  "readme": "./README.md",
  "out": "../../apps/js.wapikit.com/api-reference",
  "sourceLinkTemplate": "https://github.com/sarthakjdev/wapi.js/{path}?at={gitRevision}#line={line}",
  "sourceLinkExternal": true,
  "cname": "js.wapijs.com",
  "hideGenerator": true,
  "searchInComments": true,
  "hidePageTitle": true
}
