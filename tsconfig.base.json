{
    "compilerOptions": {
        "strictNullChecks": true,
        "moduleResolution": "node",
        "allowUnreachableCode": false,
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "declaration": true,
        "declarationMap": false,
        "importHelpers": false,
        "newLine": "lf",
        "noEmitHelpers": true,
        "noEmit": true,
        "removeComments": false,
        "sourceMap": true,
    },
    "lib": [
        "ESNext"
    ],
    "target": "ES2022"
}